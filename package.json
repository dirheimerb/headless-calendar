{
	"name": "react-headless-calendar",
	"version": "0.0.0",
	"description": "Headless calendar for React",
	"keywords": [
		"headless",
		"calendar",
		"unstyled",
		"events"
	],
	"homepage": "https://github.com/dirheimerb/react-headless-calendar#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/bdirheimer/react-headless-calendar"
	},
	"license": "MIT",
	"author": "Bradley Dirheimer",
	"exports": {
		".": {
			"import": {
				"types": "./dist/src/index.d.js",
				"default": "./dist/src/index.js"
			}
		}
	},
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.js",
	"files": [
		"/dist/src"
	],
	"scripts": {
		"build": "rm -rf ./dist && tsc",
		"build-storybook": "storybook build",
		"docs": "typedoc",
		"format": "prettier --write .",
		"prepublishOnly": "npm run build",
		"storybook": "storybook dev -p 6006",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSameLine": true,
		"bracketSpacing": true,
		"endOfLine": "auto",
		"plugins": [
			"prettier-plugin-packagejson",
			"prettier-plugin-sort-imports",
			"prettier-plugin-tailwindcss"
		],
		"printWidth": 80,
		"semi": true,
		"singleAttributePerLine": false,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"@radix-ui/react-context-menu": "^2.2.6",
		"@storybook/addon-styling-webpack": "^1.0.1"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^8.6.4",
		"@storybook/addon-essentials": "^8.6.4",
		"@storybook/addon-interactions": "^8.6.4",
		"@storybook/addon-links": "^8.6.4",
		"@storybook/addon-storysource": "^8.6.4",
		"@storybook/addon-themes": "^8.6.4",
		"@storybook/blocks": "^8.6.4",
		"@storybook/manager-api": "^8.6.4",
		"@storybook/react": "^8.6.4",
		"@storybook/react-vite": "^8.6.4",
		"@storybook/test": "^8.6.4",
		"@storybook/theming": "^8.6.4",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"autoprefixer": "^10.4.21",
		"date-fns": "^4.1.0",
		"eslint": "^9.22.0",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"framer-motion": "^12.5.0",
		"phosphor-react": "^1.4.1",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-packagejson": "^2.5.10",
		"prettier-plugin-sort-imports": "^1.8.6",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"storybook": "^8.6.4",
		"tailwindcss": "^4.0.13",
		"typedoc": "^0.27.9",
		"typedoc-plugin-github-widget": "^1.0.0",
		"typedoc-plugin-md": "^0.6.0"
	},
	"packageManager": "pnpm@9.12.1+sha256.91452fdfa46234ae447d46d5c4fc4e7e0a7058f90495c4b6f77f8beebbb154e3"
}
